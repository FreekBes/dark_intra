{
	"manifest_version": 3,
	"name": "__MSG_appName__",
	"short_name": "__MSG_shortAppName__",
	"version": "2.5.2",
	"version_name": "2.5.2",
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"icons": {
		"16": "images/logo16.png",
		"32": "images/logo32.png",
		"48": "images/logo48.png",
		"64": "images/logo64.png",
		"96": "images/logo96.png",
		"128": "images/logo128.png",
		"256": "images/logo256.png",
		"512": "images/logo512.png"
	},
	"incognito": "not_allowed",
	"options_page": "options/options.html",
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"js/inject.js",
				"css/theme-apply.css",
				"css/theme-light.css",
				"css/theme-dark.css",
				"css/colors-cetus.css",
				"css/colors-pyxis.css",
				"css/colors-vela.css"
			],
			"matches": [ "*://*.intra.42.fr/*" ]
		},
		{
			"resources": [ "options/options.html" ],
			"matches": [ "*://*.intra.42.fr/*", "*://darkintra.freekb.es/*" ]
		}
	],
	"content_scripts": [
		{
			"matches": [ "*://*.intra.42.fr/*" ],
			"css": [ "css/improv.css", "css/animations.css", "css/theme-apply.css" ],
			"js": [ "js/theme.js" ],
			"run_at": "document_start"
		},
		{
			"matches": [ "*://*.intra.42.fr/*" ],
			"js": [ "js/improv.js" ],
			"run_at": "document_end"
		},
		{
			"matches": [ "*://profile.intra.42.fr/*", "*://projects.intra.42.fr/*", "*://elearning.intra.42.fr/*", "*://meta.intra.42.fr/*", "*://shop.intra.42.fr/*" ],
			"js": [ "options/sync.js" ],
			"run_at": "document_idle"
		},
		{
			"matches": [ "*://signin.intra.42.fr/users/sign_in" ],
			"js": [ "options/unsync.js" ],
			"run_at": "document_start"
		},
		{
			"matches": [ "*://darkintra.freekb.es/connect.php*" ],
			"js": [ "options/auth.js" ],
			"run_at": "document_end"
		},
		{
			"matches": [ "*://profile.intra.42.fr/", "*://*.intra.42.fr/users/*" ],
			"js": [ "js/profiles.js" ],
			"run_at": "document_idle"
		},
		{
			"matches": [ "*://profile.intra.42.fr/", "*://*.intra.42.fr/users/*" ],
			"js": [ "campus_specifics/codam/monit.js", "campus_specifics/codam/coa-titles.js" ],
			"css": [ "campus_specifics/codam/monit.css" ],
			"run_at": "document_idle"
		},
		{
			"matches": [
				"https://projects.intra.42.fr/projects/cub3d",
				"https://projects.intra.42.fr/cub3d/*",
				"https://projects.intra.42.fr/projects/minirt",
				"https://projects.intra.42.fr/minirt/*",
				"https://projects.intra.42.fr/projects/so_long",
				"https://projects.intra.42.fr/so_long/*",
				"https://projects.intra.42.fr/projects/42cursus-fdf",
				"https://projects.intra.42.fr/42cursus-fdf/*",
				"https://projects.intra.42.fr/projects/42cursus-fract-ol",
				"https://projects.intra.42.fr/42cursus-fract-ol/*"
			],
			"js": [ "js/mlx42.js" ],
			"run_at": "document_end"
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}
